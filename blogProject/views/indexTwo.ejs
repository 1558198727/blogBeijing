<script type="text/javascript">
    $(".index_arc_item").click(function () {
        var Id = $(this).children(".date_hits").children("span").html();
//        alert(Id);
        location.href = "/blogDetail/"+"?Id="+Id;
//        $(this).children("h4").children("a").html()+
    })

    $(".panel-body").children("h2").click(function () {
        var Id = $(this).attr("id");
        location.href = "/blogDetail/"+"?Id="+Id;
    });
    $("ul").children("li").children("a").click(function () {
        var Id = $(this).attr("id");
        if(Id === undefined){
            //打开文件夹
        }else{
            location.href = "/blogDetail/"+"?Id="+Id;
        }


    })
</script>
