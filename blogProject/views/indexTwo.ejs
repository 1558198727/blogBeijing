<script>
    window.onload = function () {
        $("title").html("<%=title%>");
//当前窗口得到焦点
        window.onfocus = function() {
            $("title").html("诶，又好了");
            setTimeout(function () {
                $("title").html("<%=title%>");
            },300)
        };
        //当前窗口失去焦点
        window.onblur = function() {
            $("title").html("居然崩溃了！");
            //暂停动画或视频
        };
    }
</script >

