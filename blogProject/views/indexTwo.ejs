                <!--<div class="col-xs-12">-->

                <div class="container" >
                    <div class="row">
                        <div class="col-xs-12">

                            <div class="timeline animated">
                                <%
                                for(var i=0 ;i < data.length ;i ++){
                                var iClass = i % 4;
                                switch (iClass){
                                case 0:
                                iClass ="fa fa-pencil";
                                break;
                                case 1:
                                iClass ="fa fa-quote-right";
                                break;
                                case 2:
                                iClass ="fa fa-camera";
                                break;
                                case 3:
                                iClass ="fa fa-video-camera";
                                break;

                                default:break;
                                }
                                %>
                                <div class="timeline-row">
                                    <div class="timeline-time"><small>Oct 30</small><%=data[i].blogDate%></div>
                                    <div class="timeline-icon">
                                        <div class="bg-primary"><i class="<%=iClass%>"></i></div>
                                    </div>
                                    <div class="panel timeline-content">
                                        <div class="panel-body">
                                            <h2 id="<%=data[i].blogId %>"><%=data[i].blogTitle%></h2>
                                            <p><%=data[i].blogAnnotation%>
                                        </div>
                                    </div>
                                </div>
                                <% }%>

                            </div>

